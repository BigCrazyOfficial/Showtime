{
  "schemaVersion": 1,
  "id": "showtime",
  "version": "${version}",
  "name": "Showtime",
  "description": "Throw balls, bounce around, and just generally make a mess. Made for ModFest: Toybox.",
  "authors": ["BigCrazy"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/showtime/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "io.github.bigcrazyofficial.showtime.client.ShowtimeDataGenerator"
    ],
    "client": [
      "io.github.bigcrazyofficial.showtime.client.ShowtimeClient"
    ],
    "main": [
      "io.github.bigcrazyofficial.showtime.Showtime"
    ]
  },
  "mixins": [
    "showtime.mixins.json",
    {
      "config": "showtime.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
